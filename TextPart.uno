			cameraOffset += (float)Uno.Application.Current.FrameInterval*speed;
			//textShowed += (float)Uno.Application.FrameInterval;
			textShowed += (float)AnimTime - lasttime;

			if(textShowed>3.0f*(float)texts.Length)
			{
				textShowed-=(3.0f*(float)texts.Length);
			}
			if(textShowed>3.0f*(float)texts.Length)
			{
				textShowed = 0f;
			}

			if(cameraOffset>cubesPerBatch)
			{
				cameraOffset -= cubesPerBatch/cubeLength;
			}
			for(int i = 0; i < surroundingCircleBatches; i++)
			{
				float offset = (((float)cubesPerBatch*(i))/cubeLength);
				float lengthOfaSection = (float)cubesPerBatch/cubeLength;
				draw DefaultShading, surroundingBatch
				{
					DiffuseMap: DiffuseTex;
					//DiffuseColor: float3(1,0,0);
					DiffuseColor: Attrib2.XYZ;
					VertexPosition:
						Vector.Transform(
							(prev*cubeScale*(Attrib0.Y+0.5f)*scaleRandomScale),
							Quaternion.RotationAxis(Vector.Normalize(Attrib1.XYZ), AnimTime*Attrib1.W))
						+ float3(
							Math.Sin((Attrib0.X/cubeSpacing)+(Attrib0.Z-0.5f)*positionRandomScale)*circleSize,
							Attrib0.X/cubeLength,
							Math.Cos((Attrib0.X/cubeSpacing)+(Attrib0.W-0.5f)*positionRandomScale)*circleSize)
							+ float3(0,offset*1f,0) + float3(0,cameraOffset,0) - float3(0,lengthOfaSection*2,0);

					apply virtual Context.Pass;
				};
			}
			//End animation
			int mainText = (int)((textShowed)/3.0f);
			float mainScale = Math.Clamp((textShowed - (float)(mainText*3)), 0.0f, 1.0f);
			draw DefaultShading, textBatches[Math.Clamp(mainText, 0, texts.Length)]
			{
				DiffuseMap: DiffuseTex;
				DiffuseColor: textColor;
				VertexPosition: (prev*textCubeScale*mainScale) + (float3(Attrib0.X*textCubeSpacing, 0, Attrib0.Y*textCubeSpacing*-1f)) - (float3(Attrib0.Z, 0, Attrib0.W)*textCubeSpacing) + float3(0,0,textYOffset);
				apply virtual Context.Pass;
			};

			int nextOne = mainText-1;
			if(nextOne<0)
			{
				nextOne = texts.Length-1;
			}
			float nextScale = 1 - Math.Clamp((textShowed - (float)(mainText*3)), 0, 1);
			draw DefaultShading, textBatches[nextOne]
			{
				DiffuseMap: DiffuseTex;
				DiffuseColor: textColor;
				VertexPosition: (prev*textCubeScale*nextScale) + (float3(Attrib0.X*textCubeSpacing, 0, Attrib0.Y*textCubeSpacing*-1f)) - (float3(Attrib0.Z, 0, Attrib0.W)*textCubeSpacing) + float3(0,0,textYOffset);
				apply virtual Context.Pass;
			};

			lasttime = AnimTime;
		}
	}
}