		private int[] sizes = new int[]{121, 121, 121, 121, 121, 121, 121, 121, 121, 121, 121, 121, 121, 121, 121};

		private Batch[] textBatches;

		protected override void OnInitialize()
		{
			loadThings();
			generate();
		}
		public void loadThings()
		{
			textBatches = new Batch[texts.Length];
			var cube = Uno.Content.Models.MeshGenerator.CreateCube(float3(0,0,0), 0.5f);

			for(int i = 0; i < texts.Length; i++)
			{
				int cubeCount = 0;
				for(int a = 0; a < texts[i].Length; a++)
				{
					if(texts[i].Substring(a, 1)=="%"||texts[i].Substring(a, 1)=="1")
					{
							cubeCount++;
					}
				}
				textBatches[i] = new Batch(cubeCount*cube.Positions.Count, cubeCount*cube.Indices.Count, false);
				int indiceOffset = 0;
				for(int a = 0; a < texts[i].Length; a++)
				{
					if(texts[i].Substring(a, 1)=="%"||texts[i].Substring(a, 1)=="1")
					{
						float4 attrib0 = float4(Math.Floor(a%sizes[i]), Math.Floor(a/sizes[i]), Math.Floor(sizes[i]/2), Math.Floor((texts[i].Length/sizes[i])/2)*-1f);

						for(int b = 0; b < cube.Positions.Count; b++)
						{
							textBatches[i].Positions.Write(cube.Positions.GetFloat4(b).XYZ);
							textBatches[i].Normals.Write(cube.Normals.GetFloat4(b).XYZ);
							textBatches[i].TexCoord0s.Write(cube.TexCoords.GetFloat4(b).XY);
							textBatches[i].Attrib0Buffer.Write(attrib0);
						}
						for(int b = 0; b < cube.Indices.Count; b++)
						{
							textBatches[i].Indices.Write((ushort)(cube.Indices.GetInt(b)+indiceOffset));
						}
						indiceOffset += cube.Positions.Count;
					}
				}
			}
		}
		int cubesPerBatch = 0;
		public void generate()
		{
			const int maxVertices = 65535-100;
			Random rand = new Random(69);
			var cube = Uno.Content.Models.MeshGenerator.CreateCube(float3(0,0,0), 1f);

			cubesPerBatch = maxVertices / cube.Positions.Count;
			int verticesPerBatch = cubesPerBatch * cube.Positions.Count;
			int indicesPerBatch = cubesPerBatch * cube.Indices.Count;

			surroundingBatch = new Batch(verticesPerBatch, indicesPerBatch, true);

			float3 center = float3(0,0,0);

			int offsetVal = 0;
			var random = new Random(10);

			int indexAdd = 0;
			for(int b = 0; b < cubesPerBatch; b++)
			{
				float4 attribVal = float4(Math.Floor(offsetVal), random.NextFloat(), random.NextFloat(), random.NextFloat());
				float4 attrib1Val = float4(random.NextFloat(), random.NextFloat(), random.NextFloat(), random.NextFloat());
				float4 attrib2Val = float4(0,1,0,1);
					int r = rand.NextInt(4);
					if(r==0)
					{
						attrib2Val = float4(wallColor1, 0);
						//attrib2Val = float4(1,0,0, 0);
					}
					if(r==1)
					{
						attrib2Val = float4(wallColor2, 0);
						//attrib2Val = float4(1,0,0, 0);
					}
					if(r==2)
					{
						attrib2Val = float4(wallColor3, 0);
						//attrib2Val = float4(1,0,0, 0);
					}
					if(r==3)
					{
						attrib2Val = float4(wallColor4, 0);
						//attrib2Val = float4(1,0,0, 0);
					}
				for(int j = 0; j < cube.Positions.Count; j++)
				{
					surroundingBatch.Positions.Write(cube.Positions.GetFloat4(j).XYZ);
					surroundingBatch.Normals.Write(cube.Normals.GetFloat4(j).XYZ);
					surroundingBatch.TexCoord0s.Write(cube.TexCoords.GetFloat4(j).XY);
					surroundingBatch.Attrib0Buffer.Write(attribVal);
					surroundingBatch.Attrib1Buffer.Write(attrib1Val);
					surroundingBatch.Attrib2Buffer.Write(attrib2Val);
				}
				offsetVal++;
				for(int j = 0; j < cube.Indices.Count; j++)
				{
					surroundingBatch.Indices.Write((ushort)(cube.Indices.GetInt(j) + indexAdd));
				}
				indexAdd += cube.Positions.Count;
			}
		}
